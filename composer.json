{
    "name": "sinergi/container",
    "type": "library",
    "description": "PHP Dependency Injection container that is object oriented and IDE friendly",
    "keywords": ["Dependency", "Injection", "Container"],
    "homepage": "https://github.com/sinergi/container",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Bull",
            "email": "me@gabrielbull.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "twig/twig": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Sinergi\\Container\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sinergi\\Container\\Tests\\": ["tests/Container/Tests", "tests/Container/Tests/_includes"]
        }
    }
}
